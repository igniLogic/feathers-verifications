{
  "name": "@jordanmsllvn/feathers-verifications",
  "version": "1.0.0",
  "description": "Verification token system for FeathersJS. Enabling password reset, email verification, and beyond.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "feathersjs",
    "email verification",
    "password reset",
    "user invite"
  ],
  "author": "Jordan Sullivan <jordanmsllvn@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanmsllvn/feathers-verifications.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-mockery": "^1.2.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@feathersjs/errors": "^4.3.3",
    "@feathersjs/feathers": "^4.3.3",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/nanoid": "^2.0.0",
    "nanoid": "^2.1.1",
    "typemoq": "^2.1.0"
  }
}
